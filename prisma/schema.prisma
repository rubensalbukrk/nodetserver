// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  name String
  email String
  phone Int?
  address String
}

model Product {
  id Int @id @default(autoincrement())
  title String
  descrition String
  price Int
  descount Int
  pricebase Int
  isAvaiable Boolean
}
